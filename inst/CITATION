citHeader("To cite package 'FSAdata' in publications use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)
citEntry(entry="Manual",
         title = "FSAdata: Fisheries Stock Analysis, Datasets",
         author = personList(as.person("Derek H. Ogle")),
         year = year,
         note = note,
         textVersion = paste0("Ogle, D.H. ",year,". FSAdata: Fisheries Stock Analysis, Datasets. ",note,".")
)
